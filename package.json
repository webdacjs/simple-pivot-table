{
  "name": "simple-pivot-table",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "description": "Simple pivot table library with basica functions.",
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "http://webdacjs.github.io/simple-pivot-table",
  "dependencies": {
    "dayjs": "1.10.4",
